  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/HTTP-LRDD/v_0-102">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <rdf:Description>
            <rdfs:comment xml:lang="en">Cache host-meta files so that repeated calls to discover multiple resources on the same host do not cause repeated HTTP requests for it.</rdfs:comment>
            <rdfs:label xml:lang="en">Cache host-meta files.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Add examples to documentation.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Improved Atom support.</rdfs:label>
          </rdf:Description>
        </dcs:item>
