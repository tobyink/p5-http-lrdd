  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/HTTP-LRDD/v_0-100">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Add a parse method to actually do something with a descriptor URI once it's been found.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Add process and process_all methods which combine discover and parse stages.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Minor bugfixes and cleanups.</rdfs:label>
          </rdf:Description>
        </dcs:item>
